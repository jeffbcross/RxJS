{
  "name": "RxJS-Next",
  "version": "0.0.0-prealpha.1",
  "description": "Reactive Extensions for modern JavaScript",
  "main": "index.js",
  "scripts": {
    "build_global": "gulp build/global",
    "build_es6": "gulp build/es6",
    "build_cjs": "gulp build/cjs",
    "build_amd": "rm -rf dist/amd && babel dist/es6 --out-dir dist/amd --modules amd --sourceMaps --loose all",
    "build_test": "gulp build/cjs && jasmine",
    "test": "jasmine",
    "build_all": "npm run build_es6 && npm run build_amd && npm run build_cjs && npm run build_global",
    "perf": "protractor protractor.conf.js",
    "build_perf": "npm run build_es6 && npm run build_cjs && npm run perf"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ReactiveX/RxJS.git"
  },
  "keywords": [
    "Rx",
    "RxJS",
    "ReactiveX",
    "ReactiveExtensions",
    "Streams",
    "Observables",
    "Observable",
    "Stream",
    "ES6",
    "ES2015"
  ],
  "author": "Ben Lesh <blesh@netflix.com>",
  "license": "TBD",
  "bugs": {
    "url": "https://github.com/ReactiveX/RxJS/issues"
  },
  "homepage": "https://github.com/ReactiveX/RxJS",
  "devDependencies": {
    "babel": "^5.6.23",
    "babelify": "^6.1.2",
    "benchmark": "^1.0.0",
    "benchpress": "^2.0.0-alpha.25",
    "browserify": "^11.0.0",
    "del": "^1.2.0",
    "glob": "^5.0.10",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-insert": "^0.4.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-typescript": "^2.7.8",
    "http-server": "^0.7.5",
    "immutable": "^3.7.4",
    "jasmine": "^2.3.1",
    "jasmine-core": "^2.2.0",
    "lodash": "^3.5.0",
    "merge-stream": "^0.1.8",
    "platform": "^1.3.0",
    "protractor": "^1.7.0",
    "run-sequence": "^1.1.1",
    "rx": "^2.4.3",
    "rx-node": "^1.0.1",
    "typescript": "^1.5.3",
    "typescript-require": "^0.2.9",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
